<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="TOP_CENTER" prefHeight="260.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.winterreisender.ezzenmode.MainController">
   <MenuBar>
     <menus>
       <Menu mnemonicParsing="false" text="帮助">
         <items>
           <MenuItem fx:id="menuHomepage" mnemonicParsing="false" onAction="#onMenuHomepageClicked" text="关于" />
         </items>
       </Menu>
     </menus>
   </MenuBar>
   <TabPane prefHeight="421.0" prefWidth="693.0" tabClosingPolicy="UNAVAILABLE">
     <tabs>
       <Tab text="禅定">
         <content>
           <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                  <children>
                     <Button fx:id="startZenButton" layoutX="156.0" layoutY="85.0" mnemonicParsing="false" onAction="#onZenButtonClicked" prefHeight="36.0" prefWidth="89.0" text="开启禅定" />
                     <Label fx:id="versionInfoLabel" layoutX="185.0" layoutY="177.0" text="v0.2.1" />
                  </children>
               </AnchorPane>
         </content>
       </Tab>
     </tabs>
   </TabPane>
</VBox>
